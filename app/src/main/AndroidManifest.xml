<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.radiantridge.restoradiantridge">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MainRestoActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".FavoritesActivty" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".NearbyActivity" />
        <activity android:name=".TipCalcActivity" />
        <activity android:name=".AddRestoActivity" />
        <activity android:name=".ShowRestoActivity" />
        <activity android:name=".LatLongActivity" />
        <activity android:name=".AddReviewActivity"></activity>
    </application>

</manifest>